<p id="notice"><%= notice %></p>

<h1>Nexttimeslots</h1>

<table>
  <thead>
    <tr>
      <th>Store</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nexttimeslots.each do |nexttimeslot| %>
      <tr>
        <td><%= nexttimeslot.store_id %></td>
        <td><%= nexttimeslot.latitude %></td>
        <td><%= nexttimeslot.longitude %></td>
        <td><%= link_to 'Show', nexttimeslot %></td>
        <td><%= link_to 'Edit', edit_nexttimeslot_path(nexttimeslot) %></td>
        <td><%= link_to 'Destroy', nexttimeslot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nexttimeslot', new_nexttimeslot_path %>
